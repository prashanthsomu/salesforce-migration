<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_validation</fullName>
    <active>true</active>
    <errorConditionFormula>IF(ISPICKVAL( Status__c ,&quot;Low&quot;), true,false)</errorConditionFormula>
    <errorDisplayField>Status__c</errorDisplayField>
    <errorMessage>Account Status cannot be Low</errorMessage>
</ValidationRule>
